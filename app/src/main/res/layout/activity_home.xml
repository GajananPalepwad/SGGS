<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Home">


    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:scrollbars="none"
        android:visibility="visible"
        app:layout_constraintBottom_toTopOf="@+id/textView15"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:background="#060F20"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.5"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardView"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:backgroundTint="#060F20"
                    android:visibility="visible"
                    app:cardCornerRadius="30dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView2"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:layout_centerHorizontal="true"
                            android:layout_gravity="center"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="30dp"
                            android:elevation="12dp"
                            android:innerRadius="0dp"
                            android:shape="ring"
                            android:thicknessRatio="1.9"
                            app:cardCornerRadius="40dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">

                            <ImageView
                                android:id="@+id/profile"
                                android:layout_width="match_parent"
                                android:layout_height="60dp"
                                android:layout_alignParentTop="true"
                                android:layout_centerHorizontal="true"
                                android:layout_gravity="center"
                                android:src="@drawable/googleg_standard_color_18" />

                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/name"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginHorizontal="20dp"
                            android:text="Name"
                            android:textColor="#FFFFFF"
                            android:textSize="20dp"
                            android:textStyle="bold"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView2"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/cardView2"
                            app:layout_constraintTop_toTopOf="@+id/cardView2"
                            app:layout_constraintVertical_bias="0.09" />

                        <TextView
                            android:id="@+id/tvReg"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="3dp"
                            android:background="@drawable/roundbutton"
                            android:backgroundTint="#FDDF00"
                            android:paddingHorizontal="10dp"
                            android:paddingTop="1dp"
                            android:paddingBottom="2dp"
                            android:text="Email"
                            android:textColor="#000000"
                            app:layout_constraintStart_toStartOf="@+id/name"
                            app:layout_constraintTop_toBottomOf="@+id/name" />

                        <EditText
                            android:id="@+id/search"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20dp"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="20dp"
                            android:background="@drawable/roundbutton"
                            android:backgroundTint="#0E1D3A"
                            android:drawableStart="@android:drawable/ic_menu_search"
                            android:ems="10"
                            android:hint=" Search Anything"
                            android:inputType="text"
                            android:padding="8dp"
                            android:textColor="@color/white"
                            android:textColorHint="#6B7B99"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toStartOf="@+id/notificationBtn"
                            app:layout_constraintStart_toStartOf="parent" />

                        <ImageView
                            android:id="@+id/notificationBtn"
                            android:layout_width="52dp"
                            android:layout_height="52dp"
                            android:layout_marginEnd="20dp"
                            android:layout_marginBottom="20dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:srcCompat="@drawable/rename" />


                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="200dp">

                <TextView
                    android:id="@+id/textView10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="10dp"
                    android:text="Your Attendance"
                    android:textColor="@color/black"
                    android:textSize="20dp"
                    android:textStyle="bold"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/addCourseBtn"
                    android:layout_width="250dp"
                    android:layout_height="50dp"
                    android:background="@drawable/textviewbackground"
                    android:backgroundTint="#0E2146"
                    android:elevation="7dp"
                    android:text="Add Course"
                    android:textColor="@color/white"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="@+id/recyclerView"
                    app:layout_constraintStart_toStartOf="@+id/recyclerView"
                    app:layout_constraintTop_toTopOf="@+id/recyclerView" />


                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:divider="@null"
                    android:foregroundGravity="center_vertical"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView10"
                    app:layout_constraintVertical_bias="0.0" />
            </androidx.constraintlayout.widget.ConstraintLayout>


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <androidx.cardview.widget.CardView
                    android:id="@+id/examSection"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="10dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="10dp"
                    android:backgroundTint="#01B4B3"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView3"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:id="@+id/imageView11"
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView3"
                            app:layout_constraintEnd_toEndOf="@+id/cardView3"
                            app:layout_constraintStart_toStartOf="@+id/cardView3"
                            app:layout_constraintTop_toTopOf="@+id/cardView3"
                            app:srcCompat="@drawable/exam_logo" />

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="Exam Section"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView3"
                            app:layout_constraintStart_toStartOf="@+id/cardView3"
                            app:layout_constraintTop_toBottomOf="@+id/cardView3" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="Exam Releted\nStuff Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView11"
                            app:layout_constraintStart_toStartOf="@+id/textView11"
                            app:layout_constraintTop_toBottomOf="@+id/textView11" />


                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/notes"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="10dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="20dp"
                    android:backgroundTint="#AC14FF"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/center"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView4"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView4"
                            app:layout_constraintEnd_toEndOf="@+id/cardView4"
                            app:layout_constraintStart_toStartOf="@+id/cardView4"
                            app:layout_constraintTop_toTopOf="@+id/cardView4"
                            app:srcCompat="@drawable/notes_logo" />

                        <TextView
                            android:id="@+id/textView111"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="Notes"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView4"
                            app:layout_constraintStart_toStartOf="@+id/cardView4"
                            app:layout_constraintTop_toBottomOf="@+id/cardView4" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="Notes of Your\nclass Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView111"
                            app:layout_constraintStart_toStartOf="@+id/textView111"
                            app:layout_constraintTop_toBottomOf="@+id/textView111" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/classTime"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="20dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="10dp"
                    android:backgroundTint="#f92e5c"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintEnd_toStartOf="@+id/center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/examSection">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView5"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView5"
                            app:layout_constraintEnd_toEndOf="@+id/cardView5"
                            app:layout_constraintStart_toStartOf="@+id/cardView5"
                            app:layout_constraintTop_toTopOf="@+id/cardView5"
                            app:srcCompat="@drawable/timetable_logo" />

                        <TextView
                            android:id="@+id/textView1112"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="TimeTable"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView5"
                            app:layout_constraintStart_toStartOf="@+id/cardView5"
                            app:layout_constraintTop_toBottomOf="@+id/cardView5" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="Your Class\nTimeTable is Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView1112"
                            app:layout_constraintStart_toStartOf="@+id/textView1112"
                            app:layout_constraintTop_toBottomOf="@+id/textView1112" />


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/calender"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="20dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginEnd="10dp"
                    android:backgroundTint="#FFBB00"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/center"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/classTime">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView7"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView7"
                            app:layout_constraintEnd_toEndOf="@+id/cardView7"
                            app:layout_constraintStart_toStartOf="@+id/cardView7"
                            app:layout_constraintTop_toTopOf="@+id/cardView7"
                            app:srcCompat="@drawable/calendar_logo" />

                        <TextView
                            android:id="@+id/textView1114"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="Calendar"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView7"
                            app:layout_constraintStart_toStartOf="@+id/cardView7"
                            app:layout_constraintTop_toBottomOf="@+id/cardView7" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="Academic Calendar\nWill Show Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView1114"
                            app:layout_constraintStart_toStartOf="@+id/textView1114"
                            app:layout_constraintTop_toBottomOf="@+id/textView1114" />

                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/events"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="20dp"
                    android:backgroundTint="#00A1F1"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/center"
                    app:layout_constraintTop_toBottomOf="@+id/notes">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView6"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView6"
                            app:layout_constraintEnd_toEndOf="@+id/cardView6"
                            app:layout_constraintStart_toStartOf="@+id/cardView6"
                            app:layout_constraintTop_toTopOf="@+id/cardView6"
                            app:srcCompat="@drawable/event_logo" />

                        <TextView
                            android:id="@+id/textView1113"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="Events"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView6"
                            app:layout_constraintStart_toStartOf="@+id/cardView6"
                            app:layout_constraintTop_toBottomOf="@+id/cardView6" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="Events Announcement\nWill Show Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView1113"
                            app:layout_constraintStart_toStartOf="@+id/textView1113"
                            app:layout_constraintTop_toBottomOf="@+id/textView1113" />


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>


                <androidx.cardview.widget.CardView
                    android:id="@+id/Qbank"
                    android:layout_width="0dp"
                    android:layout_height="180dp"
                    android:layout_marginVertical="20dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="20dp"
                    android:backgroundTint="#F65314"
                    app:cardCornerRadius="20dp"
                    app:cardElevation="4dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/center"
                    app:layout_constraintTop_toBottomOf="@+id/events">


                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <androidx.cardview.widget.CardView
                            android:id="@+id/cardView8"
                            android:layout_width="80dp"
                            android:layout_height="80dp"
                            android:layout_marginTop="25dp"
                            android:alpha="0.3"
                            android:elevation="0dp"
                            app:cardCornerRadius="90dp"
                            app:cardElevation="0dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <ImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            app:layout_constraintBottom_toBottomOf="@+id/cardView8"
                            app:layout_constraintEnd_toEndOf="@+id/cardView8"
                            app:layout_constraintStart_toStartOf="@+id/cardView8"
                            app:layout_constraintTop_toTopOf="@+id/cardView8"
                            app:srcCompat="@drawable/questionbank_logo" />

                        <TextView
                            android:id="@+id/textView1115"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="5dp"
                            android:text="Question Bank"
                            android:textColor="#FFFFFF"
                            android:textSize="17dp"
                            android:textStyle="bold"
                            app:layout_constraintEnd_toEndOf="@+id/cardView8"
                            app:layout_constraintStart_toStartOf="@+id/cardView8"
                            app:layout_constraintTop_toBottomOf="@+id/cardView8" />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginVertical="3dp"
                            android:text="All Previous Years\nPapers Will Show Here"
                            android:textAlignment="center"
                            android:textColor="#FFFFFF"
                            android:textSize="12dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="@+id/textView1115"
                            app:layout_constraintStart_toStartOf="@+id/textView1115"
                            app:layout_constraintTop_toBottomOf="@+id/textView1115" />


                    </androidx.constraintlayout.widget.ConstraintLayout>


                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />


            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>
    </ScrollView>

